{
  "DevOptions": {
    "DbPlatform": "NoDb",
    "EFProvider": "MSSQL"
  },
  "ConnectionStrings": {
    "EntityFrameworkConnectionString": "server=localhost\\SQLExpress;UID=youruser;PWD=yourpassword;database=yourdbname",
    "MySqlEntityFrameworkConnectionString": "Server=localhost;Database=yourdbname;Uid=youruser;Pwd=yourpassword;Charset=utf8;",
    "PostgreSqlEntityFrameworkConnectionString": "Server=localhost;Port=5432;User Id=youruser;Password=yourpassword;Database=yourdbname;"
  },

 
  "AppSettings": {
    "UseSsl": false
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
